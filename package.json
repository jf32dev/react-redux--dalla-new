{
  "name": "gs-hs-north-dallas",
  "version": "3.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "btca-client": "^2.0.4",
    "classnames": "^2.2.6",
    "dayjs": "^1.10.4",
    "env-cmd": "^10.1.0",
    "history": "4.10.1",
    "html-entities": "^2.0.2",
    "lodash": "4.17.20",
    "mobx": "6.0.4",
    "mobx-react-lite": "3.1.6",
    "mobx-react-router": "4.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-id-swiper": "2.3.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-test-renderer": "^17.0.1",
    "swiper": "4.5.0",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@craco/craco": "6.0.0",
    "@jackfranklin/test-data-bot": "^1.3.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.11",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.165",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "chalk": "^4.1.0",
    "circular-dependency-plugin": "5.2.2",
    "commitizen": "^4.2.2",
    "cz-customizable": "^6.3.0",
    "eslint-config-airbnb-typescript": "12.0.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "2.2.1",
    "sass": "^1.30.0",
    "stylelint": "13.8.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.1.2",
    "stylelint-scss": "3.18.0",
    "ts-jest": "^26.4.4",
    "zip-webpack-plugin": "^3.0.0"
  },
  "cracoConfig": "scripts/craco.config.js",
  "scripts": {
    "commit": "cz",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "test:debug": "craco --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "typecheck": "tsc --noEmit",
    "start:main": "env-cmd -f .env.main.development craco start",
    "build:main": "env-cmd -f .env.main.production craco build",
    "start:frontline": "env-cmd -f .env.frontline.development craco start",
    "build:frontline": "env-cmd -f .env.frontline.production craco build",
    "start:lending": "env-cmd -f .env.lending.development craco start",
    "build:lending": "env-cmd -f .env.lending.production craco build",
    "start:sales": "env-cmd -f .env.sales.development craco start",
    "build:sales": "env-cmd -f .env.sales.production craco build",
    "start:twm": "env-cmd -f .env.twm.development craco start",
    "build:twm": "env-cmd -f .env.twm.production craco build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/test/**",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/setupTests.ts"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/ts-jest"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./scripts/commit-rules.config.js"
    }
  }
}
